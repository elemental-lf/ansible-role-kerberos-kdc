## {{ ansible_managed }}

[kdcdefaults]
 kdc_ports = 88
 kdc_tcp_ports = 88

[realms]
{{ krb5_kdc_realm }} = {
  master_key_type = camellia256-cts-cmac 
  acl_file = /var/kerberos/krb5kdc/kadm5.acl
  dict_file = /usr/share/dict/words
  admin_keytab = /var/kerberos/krb5kdc/kadm5.keytab
  supported_enctypes = camellia256-cts:normal aes256-cts-hmac-sha384-192:normal aes256-cts-hmac-sha1-96:normal
  default_principal_flags = +preauth
 }
